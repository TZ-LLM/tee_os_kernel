#!/bin/sh
cc="@CC@"
libc_lib="@LIBDIR@"
ldso="@LDSO@"
cleared=
shared=
userlinkdir=
userlink=
target="@TARGET@"

for x ; do
    test "$cleared" || set -- ; cleared=1

    case "$x" in
        -L-user-start)
            userlinkdir=1
            ;;
        -L-user-end)
            userlinkdir=
            ;;
        -L*)
            test "$userlinkdir" && set -- "$@" "$x"
            ;;
        -l-user-start)
            userlink=1
            ;;
        -l-user-end)
            userlink=
            ;;
        crtbegin*.o)
            set -- "$@" $(dirname $0)/../../lib/crt1.o $(dirname $0)/../../lib/crti.o /home/tools/llvm/lib/clang/10.0.1/lib/aarch64-linux-ohos/clang_rt.crtbegin.o
            ;;
        crtend*.o)
            set -- "$@" /home/tools/llvm/lib/clang/10.0.1/lib/aarch64-linux-ohos/clang_rt.crtend.o $(dirname $0)/../../lib/crtn.o
            ;;
        -lgcc|-lgcc_eh)
            file=lib${x#-l}.a
            set -- "$@" -lclang_rt.builtins -L/home/tools/llvm/lib/clang/10.0.1/lib/aarch64-linux-ohos
            ;;
        -l*)
            test "$userlink" && set -- "$@" "$x"
            ;;
        -shared)
            shared=1
            set -- "$@" -shared
            ;;
        -sysroot=*|--sysroot=*)
            ;;
        *)
            set -- "$@" "$x"
            ;;
    esac
done

$($cc -print-prog-name=ld.lld) -nostdlib "$@" -lc -dynamic-linker "$ldso"